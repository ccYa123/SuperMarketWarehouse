<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="editPane" prefHeight="465.0" prefWidth="244.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EditController">
   <children>
      <ImageView fitHeight="482.0" fitWidth="281.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/EditImage.jpg" />
         </image>
      </ImageView>
      <Pane layoutY="102.0" prefHeight="380.0" prefWidth="277.0" style="-fx-background-color: white;">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="商品编号" />
            <Label layoutX="14.0" layoutY="58.0" text="商品名称" />
            <TextField fx:id="Name" layoutX="74.0" layoutY="54.0" style="-fx-background-radius: 10px;" />
            <Label layoutX="19.0" layoutY="104.0" text="单    价" />
            <TextField fx:id="Price" layoutX="74.0" layoutY="100.0" style="-fx-background-radius: 10px;" />
            <Label layoutX="19.0" layoutY="146.0" text="类    别" />
            <TextField fx:id="Kind" layoutX="74.0" layoutY="142.0" style="-fx-background-radius: 10px;" />
            <Label layoutX="19.0" layoutY="190.0" text="销    量" />
            <TextField fx:id="Sales" layoutX="74.0" layoutY="186.0" style="-fx-background-radius: 10px;" />
            <Label layoutX="19.0" layoutY="235.0" text="库    存" />
            <TextField fx:id="Sum" layoutX="74.0" layoutY="231.0" style="-fx-background-radius: 10px;" />
            <Label layoutX="14.0" layoutY="279.0" text="销售状态" />
            <TextField fx:id="Sell" layoutX="74.0" layoutY="275.0" style="-fx-background-radius: 10px;" />
            <Button layoutX="25.0" layoutY="329.0" mnemonicParsing="false" onAction="#save" prefHeight="23.0" prefWidth="74.0" style="-fx-background-color: #00BFFF; -fx-background-radius: 10px;" text="保存" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button layoutX="178.0" layoutY="329.0" mnemonicParsing="false" onAction="#delete" prefHeight="23.0" prefWidth="74.0" style="-fx-background-color: #00BFFF; -fx-background-radius: 10px;" text="删除" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label fx:id="Num" layoutX="74.0" layoutY="10.0" prefHeight="23.0" prefWidth="161.0" />
         </children>
      </Pane>
      <ImageView fitHeight="43.0" fitWidth="124.0" layoutX="76.0" layoutY="35.0" opacity="0.9" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/editMessageImage.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
